<table class="table">
  <thead>
    <tr>
      <th scope="col">Rank #</th>
      <th scope="col">Team</th>
      <th scope="col">Score</th>
    </tr>
  </thead>
  <tbody>
    <%- @teams.each do |team| %>
      <tr>
        <th scope="row"><%=team.ranking%></th>
        <td><%=team.name%></td>
        <td><%=team.score%></td>
      </tr>
    <% end %>
  </tbody>
</table>
