<%= form_with(model: match_result, local: true) do |form| %>
  <% if match_result.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(match_result.errors.count, "error") %> prohibited this match_result from being saved:</h2>

      <ul>
      <% match_result.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= form.label :winningteam_id %>
    <%= form.number_field :winningteam_id %>
  </div>

  <div class="field">
    <%= form.label :game_id %>
    <%= form.number_field :game_id %>
  </div>

  <div class="field">
    <%= form.label :score %>
    <%= form.number_field :score %>
  </div>

  <div class="field">
    <%= form.label :losingteam %>
    <%= form.number_field :losingteam %>
  </div>

  <div class="actions">
    <%= form.submit %>
  </div>
<% end %>
